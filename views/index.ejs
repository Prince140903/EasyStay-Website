<% layout("layouts/boilerplate") %>
<body>
  <!-- <h1>All Listings</h1> -->
  <br /><br />
  <div class="row">
    <% for (let list of allListing) { %>
    <a href="/listings/<%= list._id %>" class="click">
      <div class="card">
        <img src="<%= list.image %>" alt="Listing Image" class="card-img" />
        <div class="card-body">
          <p>
            <b><%= list.title %></b><br />
            &#8377; <%= list.price.toLocaleString("en-IN") %> /night
          </p>
        </div>
      </div>
    </a>
    <% } %>
  </div>
</body>
